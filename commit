package main

import (
    "fmt"
    "os"
    "runtime"
)

var _, _ = fmt.Println, os.Exit

func main() {
    
}

func printError(err error, str ...string) {
    if err != nil {
        pc, _, _, _ := runtime.Caller(1)
        fmt.Println(runtime.FuncForPC(pc).Name())
        fmt.Println(err)
        for _, s := range str {
            fmt.Println(s)
        }
    }
}

